{
    "name": "pschocke/laravel-notification-settings",
    "description": "Let users decide on what event they want to be notified over which way",
    "keywords": [
        "Laravel",
        "Laravel Notifications"
    ],
    "homepage": "https://github.com/pschocke/laravel-notification-settings",
    "license": "MIT",
    "authors": [
        {
            "name": "Patrick Schocke",
            "email": "patrick@ausbildung-ms.de",
            "homepage": "https://spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4",
        "illuminate/support": "~6|~7"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "orchestra/testbench": "^5.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "pschocke\\NotificationSettings\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "pschocke\\NotificationSettings\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "pschocke\\NotificationSettings\\NotificationSettingsServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
